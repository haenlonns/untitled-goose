{
	"manifest_version": 3,
	"name": "Untitled Goose",
	"version": "0.69",
	"description": "GOOOOOOSE",
	"content_scripts": [
		{
			"js": ["scripts/content.ts"],
			"matches": ["https://*/*", "http://*/*"]
		}
	],
	"web_accessible_resources": [
		{
			"resources": ["img/goose_chomp.gif", "img/goose_run.gif", "img/goose_idle.gif"],
			"matches": ["https://*/*", "http://*/*"]
		}
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	}
}
